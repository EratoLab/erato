{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "dev": "pnpm i18n:compile && vite",
    "build": "pnpm i18n:compile && tsc && vite build",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:strict": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "codegen:generate": "openapi-codegen gen v1betaApi",
    "codegen:fix-types": "node scripts/fix-codegen-types.mjs",
    "codegen": "pnpm codegen:generate && pnpm codegen:fix-types",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint:strict && pnpm typecheck",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile",
    "i18n:watch": "lingui extract --watch"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@lingui/core": "^5.3.2",
    "@lingui/detect-locale": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@react-hook/resize-observer": "^2.0.2",
    "@tanstack/query-sync-storage-persister": "^5.69.0",
    "@tanstack/react-query": "5.75.7",
    "@tanstack/react-query-devtools": "^5.75.7",
    "@tanstack/react-query-persist-client": "^5.75.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "non.geist": "^1.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.25.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-use": "^17.6.0",
    "react-window": "^1.8.11",
    "remark-gfm": "^4.0.1",
    "sse.js": "^2.5.0",
    "use-debounce": "^10.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.4.0",
    "@lingui/babel-plugin-lingui-macro": "^5.3.2",
    "@lingui/cli": "^5.3.2",
    "@lingui/vite-plugin": "^5.3.2",
    "@openapi-codegen/cli": "^3.1.0",
    "@openapi-codegen/typescript": "^11.0.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-lingui": "^0.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.11.2",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "postcss": "^8",
    "prettier": "3.5.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite": "^5.4.2",
    "vitest": "^3.2.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
